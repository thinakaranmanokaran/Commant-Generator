{
  "name": "command-generator",
  "displayName": "Command Generator AI",
  "description": "Generate Make/CLI commands using AI based on your code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "activationEvents": [
    "onCommand:command-generator.generate"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "command-generator.generate",
        "title": "Generate Command with AI"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "command-generator.generate",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    },
    "keybindings": [
      {
        "command": "command-generator.generate",
        "key": "ctrl+shift+c",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "test": "vscode-test"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.x",
    "@types/vscode": "^1.103.0",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.34.0"
  }
}
